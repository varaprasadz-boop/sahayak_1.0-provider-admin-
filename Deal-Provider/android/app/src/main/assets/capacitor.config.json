{
	"appId": "com.sahayak.provider",
	"appName": "Sahayak Provider",
	"webDir": "www",
	"bundledWebRuntime": false,
	"plugins": {
		"SplashScreen": {
			"launchAutoHide": false,
			"showSpinner": false,
			"androidScaleType": "CENTER_CROP"
		},
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"LocalNotifications": {
			"iconColor": "#111111",
			"sound": "beep.wav"
		}
	}
}
